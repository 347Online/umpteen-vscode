{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Umpteen",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.umpteen",
				"match": "\\b(if|return|loop|break|continue|fnc)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.umpteen",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.umpteen",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "source.umpteen"
}